    <!-- Custom JS for sidebar and select2 -->
    <script>
        $(document).ready(function() {
            // Submenu toggle with icon rotation
            $('#sidebar-menu .has-submenu > a').click(function(e) {
                e.preventDefault();
                $(this).parent().toggleClass('active');
                $(this).find('.menu-arrow i').toggleClass('mdi-rotate-90');
                $(this).next('.submenu').slideToggle(300);
            });

            // Initialize Select2
            $('.select2').select2({
                dropdownParent: $('body'),
                width: '100%'
            });

            // Enhanced Topbar menu toggle for mobile
            $('.button-menu-mobile').click(function(e) {
                e.preventDefault();
                $('body').toggleClass('enlarged');
                
                if ($(window).width() <= 768) {
                    $('.left.side-menu').toggleClass('show');
                    if ($('.left.side-menu').hasClass('show')) {
                        $('.left.side-menu').css({
                            'left': '0',
                            'display': 'block'
                        });
                    } else {
                        $('.left.side-menu').css({
                            'left': '-240px',
                            'display': 'none'
                        });
                    }
                } else {
                    if($('body').hasClass('enlarged')) {
                        $('.left.side-menu').fadeOut(300);
                        $('.content-page').animate({'margin-left': '0'}, 300);
                        $('.navbar-custom').animate({'margin-left': '0'}, 300);
                        $('.topbar-left').animate({'width': '70px'}, 300);
                    } else {
                        $('.left.side-menu').fadeIn(300);
                        $('.content-page').animate({'margin-left': '240px'}, 300);
                        $('.navbar-custom').animate({'margin-left': '240px'}, 300);
                        $('.topbar-left').animate({'width': '240px'}, 300);
                    }
                }
            });

            // Handle window resize
            function handleResize() {
                if ($(window).width() <= 768) {
                    $('body').addClass('enlarged');
                    $('.left.side-menu').css({
                        'left': '-240px',
                        'display': 'none'
                    });
                    $('.content-page').css('margin-left', '0');
                    $('.navbar-custom').css('margin-left', '0');
                    $('.topbar-left').css('width', '137px');
                } else {
                    $('body').removeClass('enlarged');
                    $('.left.side-menu').css({
                        'left': '0',
                        'display': 'block'
                    });
                    $('.content-page').css('margin-left', '240px');
                    $('.navbar-custom').css('margin-left', '240px');
                    $('.topbar-left').css('width', '240px');
                }
            }

            // Initial check and bind resize event
            handleResize();
            $(window).resize(handleResize);
        });
    </script>
